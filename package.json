{
  "name": "react-rsql-query-builder",
  "version": "3.4.7-dev",
  "description": "Customizable RSQL front-end lib for React",
  "author": "Jo√£o Alex <joao.alex.apps@gmail.com>",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./style": "./dist/react-rsql-query-builder.css",
    "./styles.css": "./dist/react-rsql-query-builder.css"
  },
  "license": "MIT",
  "keywords": [
    "rsql",
    "fiql",
    "react",
    "filter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Joao-ava/react-rsql-query-builder"
  },
  "bugs": {
    "url": "https://github.com/Joao-ava/react-rsql-query-builder/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 --cache --cache-location ./node_modules/.tmp/.eslint/",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.12",
    "@rsql/ast": "^1.6.0",
    "@rsql/builder": "^1.6.0",
    "i18next": "^22.5.1",
    "motion": "^11.13.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-links": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.6.0",
    "ajv": "^8.17.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.2",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "storybook": "^9.1.2",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "lint-staged": {
    "*.{js, ts,tsx}": [
      "npm run format",
      "npm run lint"
    ],
    "*.{css,md}": "npm run format"
  }
}
